<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Contact Details
          <button class="btn btn-md" ng-click="editUserForm()">
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>Edit</button>
          <button class="btn btn-md" ng-click="deleteUser(userObj._id)">
          <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>Delete</button>
        </h3>
    </div>
    <div class="panel-body">
      <div class="row">
        <div class="col-md-8 col-md-offset-2 " align="center"> <img alt="User Pic" src="http://www.w3schools.com/howto/img_avatar.png" class="img-circle img-responsive" width="20%" height ="20%"> </div>
      </div>
      </br>
      <div class="col-md-8 col-md-offset-2">
          <table class="table table-user-information">
              <tbody>
                <tr>
                  <td>Name:</td>
                  <td>{{userObj.name.title}} {{userObj.name.first}} {{userObj.name.last}}</td>
                </tr>
                <tr>
                  <td>Gender:</td>
                  <td>{{userObj.gender}}</td>
                </tr>
                <tr>
                  <td>Date of Birth</td>
                  <td>{{userObj.dob}}</td>
                </tr>
                <tr>
                  <td>Email</td>
                  <td>{{userObj.email}}</td>
                </tr>
                <tr>
                  <td>Phone</td>
                  <td>{{userObj.phone}}</td>
                </tr>
                <tr>
                  <td>Cell</td>
                  <td>{{userObj.cell}}</td>
                </tr>
              </tbody>
          </table>
      </div>
    </div>
</div>
